html,body,#viewport,#viewport > div
    reset-box-model()
    reset-font()
    reset-list()
    height:100%


.slider
    overflow:hidden;
    height:100%
    
    

.slider > div > div:nth-of-type(1).test
    background-color:red

.slider > div > div:nth-of-type(2)
    background-color:blue
    
.slider > div > div:nth-of-type(3)
    background-color:green

.slider > div > div:nth-of-type(4)
    background-color:grey